    <loc>http://hui.us.com/{{ post.url }}</loc>
    <lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
  </url>
  {% endfor %}
  
  <url>
      <loc>http://hui.us.com/</loc>

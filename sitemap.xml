    <changefreq>weekly</changefreq>
  </url>
  <url>
    <loc>http://hui.us.com/about/</loc>
    <lastmod>2010-01-14</lastmod>
  </url>
  <url>
    <loc>http://hui.us.com/archive/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
  </url>
</urlset> 
